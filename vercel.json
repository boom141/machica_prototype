{
    "version": 2,

    "builds": [
        {
            "src": "main.py",
            "use": "@vercel/python"
        }
    ],

    "routes": [
        {
            "src": "(.*)",
            "dest": "main.py"
        }
    ],
    "functions": {
        "admin.py": {
          "memory": 3008,
          "maxDuration": 30
        }
    }
}
