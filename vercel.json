{
    "version": 2,
    "builds": [
        {
            "src": "*.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "(.*)",
            "dest": "main.py"
        }
    ],
    "function": {
        "admin.py": {
          "memory": 3008,
          "maxDuration": 30
        }
    }
 }
